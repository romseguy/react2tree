{
  "name": "react2tree",
  "version": "1.0.0",
  "description": "Utility for mapping React component hierarchies to trees",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/react2tree.js && NODE_ENV=production webpack src/index.js dist/react2tree.min.js",
    "lint": "eslint src examples",
    "test": "NODE_ENV=test mocha",
    "test:watch": "NODE_ENV=test mocha --watch",
    "test:cov": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha",
    "prepublish": "npm run lint && npm run clean && npm run build && npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/romseguy/react2tree.git"
  },
  "keywords": [
    "react",
    "tree",
    "mapping",
    "transformation"
  ],
  "author": "romseguy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/romseguy/react2tree/issues"
  },
  "homepage": "https://github.com/romseguy/react2tree",
  "devDependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.6.18",
    "babel-eslint": "^3.1.15",
    "babel-loader": "^5.1.4",
    "eslint": "^0.23",
    "eslint-config-airbnb": "0.0.6",
    "eslint-plugin-react": "^2.3.0",
    "expect": "^1.6.0",
    "isparta": "^3.0.3",
    "mocha": "^2.2.5",
    "rimraf": "^2.3.4",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "react": "^0.13.3"
  }
}
